<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.example.footballdatastore">

<uses-permission android:name="android.permission.INTERNET" />

<application
    android:allowBackup="true"
    android:icon="@drawable/logo"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".PlayerSearch"></activity>
    <activity android:name=".TeamSearch" />
    <activity android:name=".PlayerSort" />
    <activity android:name=".TeamSort" />
    <activity android:name=".Players" />
    <activity android:name=".Teams" />
    <activity android:name=".HomepageActivity" />
    <activity android:name=".LoginActivity" />
    <activity android:name=".MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
</application>

</manifest>